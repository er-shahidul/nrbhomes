{% extends 'UserBundle::layout.html.twig' %}

{% block title %}{{ 'Users'|trans }} | {{ parent() }}{% endblock %}
{% block pageTitle %}<h1>{{ 'Users'|trans }}</h1>{% endblock %}


{% block body %}
    <!-- BEGIN PAGE BREADCRUMB -->
    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="{{ path('dashboard') }}">{{ 'Dashboard'|trans }}</a><i class="fa fa-circle"></i>
        </li>
        <li>
            <a href="">{{ 'Users'|trans }}</a><i class="fa fa-circle"></i>
        </li>
        {#<li class="active"> {{ mode == 'create' ? 'Create'|trans : 'Update'|trans }} {{ 'User'|trans }}</li>#}
    </ul>
    <!-- END PAGE BREADCRUMB -->
    <!-- BEGIN PAGE CONTENT INNER -->
    <div class="row" id="volume-list-page">
        <div class="col-sm-12">
            <div class="portlet light">
                <div class="portlet-title">
                    <div class="caption caption-md">
                        <i class="icon-bar-chart theme-font hide"></i>
                        <span class="caption-subject theme-font bold uppercase">{{ 'User'|trans }}</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div class='table-data'>
                        {% include "UserBundle:Registration:register_content.html.twig" %}
                    </div>
                </div>
            </div>
        </div>

    </div>
    <!-- END PAGE CONTENT INNER -->
{% endblock %}
